{
  "name": "dtsse-ardoq-adapter",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "eslint . && prettier --check .",
    "lint:fix": "prettier --write . && eslint --fix .",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit --detectOpenHandles",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run --steps",
    "cichecks": "yarn build && yarn lint && yarn test && yarn test:routes√ò",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@hmcts/nodejs-healthcheck": "1.8.5",
    "@hmcts/nodejs-logging": "4.0.4",
    "@hmcts/properties-volume": "1.1.0",
    "applicationinsights": "3.1.0",
    "axios": "1.7.2",
    "axios-request-throttle": "^1.0.0",
    "body-parser": "1.20.2",
    "compression": "^1.7.4",
    "config": "3.3.11",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "dotenv": "^16.0.3",
    "express": "4.19.2",
    "express-nunjucks": "3.1.2",
    "express-openapi-validator": "^5.0.3",
    "glob": "10.4.2",
    "govuk-frontend": "5.4.0",
    "helmet": "7.1.0",
    "jquery": "3.7.1",
    "lodash": "4.17.21",
    "nunjucks": "3.2.4",
    "require-directory": "2.1.1",
    "serve-favicon": "2.5.0",
    "snyk-nodejs-lockfile-parser": "^1.57.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typed-rest-client": "^1.8.9",
    "typescript": "5.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.24.7",
    "@babel/eslint-parser": "7.24.7",
    "@babel/preset-env": "7.24.7",
    "@codeceptjs/configure": "^1.0.1",
    "@jest/globals": "^29.3.1",
    "@types/body-parser": "1.19.5",
    "@types/chai-as-promised": "7.1.8",
    "@types/compression": "^1.7.2",
    "@types/config": "3.3.4",
    "@types/cookie-parser": "1.4.7",
    "@types/csurf": "1.11.5",
    "@types/es6-promisify": "6.0.4",
    "@types/express": "4.17.21",
    "@types/glob": "8.1.0",
    "@types/jest": "^29.2.6",
    "@types/lodash": "4.17.5",
    "@types/mini-css-extract-plugin": "2.5.1",
    "@types/node": "20.14.7",
    "@types/nunjucks": "3.2.6",
    "@types/require-directory": "2.1.6",
    "@types/serve-favicon": "2.5.7",
    "@types/supertest": "2.0.16",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "axios-debug-log": "1.0.0",
    "babel-loader": "9.1.3",
    "chai": "5.1.1",
    "chai-as-promised": "8.0.0",
    "chokidar": "3.6.0",
    "codeceptjs": "^3.3.6",
    "copy-webpack-plugin": "12.0.2",
    "css-loader": "7.1.2",
    "debug": "4.3.5",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jest": "28.6.0",
    "html-webpack-plugin": "5.6.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-html-reporter": "3.10.2",
    "jest-junit": "16.0.0",
    "jest-mock": "^29.3.1",
    "lint-staged": "15.2.7",
    "mini-css-extract-plugin": "2.9.0",
    "nock": "13.5.4",
    "nodemon": "3.1.4",
    "playwright": "^1.18",
    "prettier": "^3.3.2",
    "sass": "1.77.6",
    "sass-loader": "14.2.1",
    "sinon": "15.2.0",
    "sinon-chai": "3.7.0",
    "style-loader": "3.3.3",
    "supertest": "^6.3.1",
    "ts-jest": "29.1.5",
    "ts-loader": "9.5.1",
    "webpack": "5.92.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-middleware": "7.2.1",
    "webpack-node-externals": "3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "**/*.{js,ts}": "eslint --fix"
  },
  "packageManager": "yarn@3.6.4"
}
